type Node {
  value: String
  next: Node? = None

  func toString(self): String {
    var str = self.value + ", "
    var next = self.next

    while (next != None) {
      str = str + (next?.value ?: "") + ", "

      next = next?.next
    }

    str[:-2]
  }
}

val node = Node(
  value: "a",
  next: Node(
    value: "b",
    next: Node(
      value: "c",
    )
  )
)
node.toString()
