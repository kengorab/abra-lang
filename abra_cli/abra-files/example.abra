func foo1(v: Int, x = 123): Int[] {
  [v, x]
}
println(foo1(1))
println(foo1(1, 2))

func foo2(*args: Int[]) {
  val [a, b, c] = args
  println(a, b, c)
}
foo2(1, 2, 3, 4)

