type Person {
  firstName: String
  lastName: String

  func fullName(self) {
    self.firstName + " " + self.lastName
  }

  func ken() = "Ken"
}

val people = [
  Person(firstName: "Ken", lastName: "Gorab"),
  Person(firstName: "Meg", lastName: "Muccilli"),
]

(people[0] ?: Person(firstName: "unknown", lastName: "person")).fullName()
