/// Expect: 24
echo<Int>(24)

// Binary operations
/// Expect: 32
echo<Int>(17 + 15)
/// Expect: 32.1
echo<Float>(17 + 15.1)

/// Expect: 2
echo<Int>(17 - 15)
/// Expect: 1.9
echo<Float>(17 - 15.1)

/// Expect: 2550
echo<Int>(17 * 150)
/// Expect: 2551.7
echo<Float>(17 * 150.1)

/// Expect: 1.13333
echo<Float>(17 / 15)
/// Expect: 1.12583
echo<Float>(17 / 15.1)

/// Expect: true
echo<Bool>(17 == (15 + 2))
/// Expect: false
echo<Bool>(17 == 15)
/// Expect: true
echo<Bool>(17 == (15 + 2.0))
/// Expect: false
echo<Bool>(17 == 15.1)

/// Expect: false
echo<Bool>(17 != (15 + 2))
/// Expect: true
echo<Bool>(17 != 15)
/// Expect: false
echo<Bool>(17 != (15 + 2.0))
/// Expect: true
echo<Bool>(17 != 15.1)

/// Expect: true
echo<Bool>(17 < 18)
/// Expect: false
echo<Bool>(17 < 15)
/// Expect: true
echo<Bool>(17 < 18.0)
/// Expect: false
echo<Bool>(17 < 15.1)

/// Expect: false
echo<Bool>(17 > 18)
/// Expect: true
echo<Bool>(17 > 15)
/// Expect: false
echo<Bool>(17 > 18.0)
/// Expect: true
echo<Bool>(17 > 15.1)

/// Expect: true
echo<Bool>(17 <= 18)
/// Expect: false
echo<Bool>(17 <= 15)
/// Expect: true
echo<Bool>(17 <= 18.0)
/// Expect: false
echo<Bool>(17 <= 15.1)

/// Expect: false
echo<Bool>(17 >= 18)
/// Expect: true
echo<Bool>(17 >= 15)
/// Expect: false
echo<Bool>(17 >= 18.0)
/// Expect: true
echo<Bool>(17 >= 15.1)
