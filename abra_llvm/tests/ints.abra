/// Expect: 24
println(24)

// Unary operations
/// Expect: -1 1
println(-1, --1)

// Binary operations
/// Expect: 32
println(17 + 15)
/// Expect: 32.1
println(17 + 15.1)

/// Expect: 2
println(17 - 15)
/// Expect: 1.9
println(17 - 15.1)

/// Expect: 2550
println(17 * 150)
/// Expect: 2551.7
println(17 * 150.1)

/// Expect: 1.13333
println(17 / 15)
/// Expect: 1.12583
println(17 / 15.1)

/// Expect: 2
println(7 % 5)
/// Expect: 1.8
println(7 % 5.2)

/// Expect: 32
println(2 ** 5)
/// Expect: 0.03125
println(2 ** -5)
/// Expect: -32
println(-2 ** 5)
/// Expect: -0.03125
println(-2 ** -5)

/// Expect: true
println(1 << 3 != 17)
/// Expect: true
println(0x00ff << 8 == 0xff00)
/// Expect: true
println(0xff00 >> 8 == 0x00ff)

/// Expect: 1
println(0xff && 1)
/// Expect: 255
println(0xfe || 1)
/// Expect: 255
println(0b01010101 ^ 0b10101010)

/// Expect: true
println(17 == (15 + 2))
/// Expect: false
println(17 == 15)
/// Expect: true
println(17 == (15 + 2.0))
/// Expect: false
println(17 == 15.1)

/// Expect: false
println(17 != (15 + 2))
/// Expect: true
println(17 != 15)
/// Expect: false
println(17 != (15 + 2.0))
/// Expect: true
println(17 != 15.1)

/// Expect: true
println(17 < 18)
/// Expect: false
println(17 < 15)
/// Expect: true
println(17 < 18.0)
/// Expect: false
println(17 < 15.1)

/// Expect: false
println(17 > 18)
/// Expect: true
println(17 > 15)
/// Expect: false
println(17 > 18.0)
/// Expect: true
println(17 > 15.1)

/// Expect: true
println(17 <= 18)
/// Expect: false
println(17 <= 15)
/// Expect: true
println(17 <= 18.0)
/// Expect: false
println(17 <= 15.1)

/// Expect: false
println(17 >= 18)
/// Expect: true
println(17 >= 15)
/// Expect: false
println(17 >= 18.0)
/// Expect: true
println(17 >= 15.1)

// Int#abs
/// Expect: 12
println(12.abs())
/// Expect: 12
println((-12).abs())

// Int#asBase
/// Expect: None None None None
println(123.asBase(1), 123.asBase(0), 123.asBase(-1), 123.asBase(37))
/// Expect: 1100 -1100
println(12.asBase(2), (-12).asBase(2))
/// Expect: 4d2 -4d2
println(1234.asBase(16), (-1234).asBase(16))
/// Expect: ya -ya
println(1234.asBase(36), (-1234).asBase(36))
/// Expect: L8WN -L8WN
println(11235813.asBase(62), (-11235813).asBase(62))

// Int#hex
/// Expect: 0x2a 0x4d2
println(42.hex(), 1234.hex())

// Int#isEven
/// Expect: true true
println(12.isEven(), (-12).isEven())
/// Expect: false false
println(11.isEven(), (-11).isEven())

// Int#isOdd
/// Expect: false false
println(12.isOdd(), (-12).isOdd())
/// Expect: true true
println(11.isOdd(), (-11).isOdd())

// Int#isBetween
/// Expect: true false false false false
println(12.isBetween(10, 14), (-12).isBetween(10, 14), 10.isBetween(10, 14), 14.isBetween(10, 14), 15.isBetween(10, 14))
/// Expect: false true true false
println((-12).isBetween(10, 14, true), 10.isBetween(10, 14, true), 14.isBetween(10, 14, true), 15.isBetween(10, 14, true))
