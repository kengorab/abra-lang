func f1(): Bool?[] = [None, Option.Some(value: true), Option.Some(value: false)]
/// Expect: [Option.None, Option.Some(value: true), Option.Some(value: false)]
println(f1())

func f2(): String?[] = if true { [Option.Some(value: "one"), None, Option.Some(value: "two")] } else []
/// Expect: [Option.Some(value: "one"), Option.None, Option.Some(value: "two")]
println(f2())

val arr1: Int?[] = [Option.Some(value: 1), None, Option.Some(value: 2), Option.Some(value: 3)]
/// Expect: [Option.Some(value: 1), Option.None, Option.Some(value: 2), Option.Some(value: 3)]
println(arr1)

val arr2: Int[]? = Option.Some(value: [1, 2, 3])
/// Expect: Option.Some(value: [1, 2, 3])
println(arr2)
val arr3: Int[]? = None
/// Expect: Option.None
println(arr3)

type Foo {
  f: Int
}

// Workaround because optionals technically don't have _any_ directly-callable methods
func hash<T>(t: T): Int = t.hash()
// Hashes
(() => {
  val i: Int? = None
  /// Expect: 8
  println(hash(i))

  val x: Int? = Option.Some(value: 17)
  /// Expect: false
  println(hash(x) == (17).hash())
})()

// == operator
(() => {
  // Test with primitive
  val i1: Int? = None
  val i2: Int? = Option.Some(value: 17)
  val i3: Int? = Option.Some(value: 17)
  val i4: Int? = Option.Some(value: 16)

  /// Expect: false
  println(i1 == i2)
  /// Expect: false
  println(i2 == i4)
  /// Expect: true
  println(i2 == i3)

  // Test with object
  val s1: String? = None
  val s2: String? = Option.Some(value: "str")
  val s3: String? = Option.Some(value: "str")
  val s4: String? = Option.Some(value: "hmm")

  /// Expect: false
  println(s1 == s2)
  /// Expect: false
  println(s2 == s4)
  /// Expect: true
  println(s2 == s3)
})()

// Coalescing (?:)
(() => {
  val i1: Int? = None
  /// Expect: 17
  println(i1 ?: 17)

  val i2: Int? = Option.Some(value: 9)
  /// Expect: 9
  println(i2 ?: 17)
})()

val xyz = "abc"
type TestOptChainingMethodInvocation {
  func unitMethod(self) = println("TestOptChainingMethodInvocation#unitMethod")
  func closure(self): Int = xyz.length
}

// Optional-chaining accessor
(() => {
  val arr = ["a", "b", "c", "d"]

  // Getting field of known-non-None value
  /// Expect: 4
  println(arr?.length)
  // Calling method of known-non-None value
  /// Expect: [a, b, c, d]
  println(arr?.toString())

  // Getting field of Option value
  /// Expect: Option.Some(value: 1)
  println(arr[0]?.length)
  /// Expect: Option.None
  println(arr[6]?.length)

  // Calling method of Option value
  /// Expect: Option.Some(value: false)
  println(arr[0]?.isEmpty())
  /// Expect: Option.None
  println(arr[6]?.isEmpty())

  // Calling unit method of Option value
  val t1: TestOptChainingMethodInvocation? = None
  t1?.unitMethod()
  val t2: TestOptChainingMethodInvocation? = Option.Some(value: TestOptChainingMethodInvocation())
  /// Expect: TestOptChainingMethodInvocation#unitMethod
  t2?.unitMethod()

  // Calling closure method of Option value
  val t3: TestOptChainingMethodInvocation? = None
  /// Expect: Option.None
  println(t3?.closure())
  val t4: TestOptChainingMethodInvocation? = Option.Some(value: TestOptChainingMethodInvocation())
  /// Expect: Option.Some(value: 3)
  println(t4?.closure())
})()

