// Default-valued parameters
func foo(a: Int, b = bar(a), c = bar(a, b), arr = [a, b, c]): Int = a + b + c + arr.length
func bar(x: Int, y = 10): Int = x + y
/// Expect: 31
println(foo(2))

// Variadic parameters
func variadic(*items: Int[]) = println(items)
/// Expect: [1, 2, 3]
variadic(1, 2, 3)
/// Expect: []
variadic()
