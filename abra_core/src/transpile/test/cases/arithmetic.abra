val six = 6
val twentyFour = 24

val sixPointOne = 6.1
val twentyFourPointTwo = 24.2

// Test binary operators
println(
  six + twentyFour,
  sixPointOne + twentyFourPointTwo,
  sixPointOne + twentyFour,
  six + twentyFourPointTwo
)

println(
  six - twentyFour,
  sixPointOne - twentyFourPointTwo,
  sixPointOne - twentyFour,
  six - twentyFourPointTwo
)

println(
  six * twentyFour,
  sixPointOne * twentyFourPointTwo,
  sixPointOne * twentyFour,
  six * twentyFourPointTwo
)

println(
  six / twentyFour,
  sixPointOne / twentyFourPointTwo,
  sixPointOne / twentyFour,
  six / twentyFourPointTwo
)

println(
  six ** twentyFour,
  sixPointOne ** twentyFourPointTwo,
  sixPointOne ** twentyFour,
  six ** twentyFourPointTwo
)

// Test unary operator
println(
  -(-six + -twentyFourPointTwo)
)

