val hello = "hello"
val world = "world"

println(hello, world)

println(hello + " " + world + "!")

// Test equality operators
println("hello" == "hello", "hello" == "world")
println("hello" != "hello", "hello" != "world")

// Indexing
val str = "hello"
println(str[0], str[-4], str[-8], str[8])
println(str[1:4], str[-4:4])
println(str[1:], str[:1])

