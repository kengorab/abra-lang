// Tuples: for loops
(() => {
  val items = [(1, 2), (3, 4), (5, 6)]
  for (a, b) in items print(a + b)
  println()
})()

// Tuples: ifs
(() => {
  val items = [(1, 2)]
  if items[0] |(a, b)| println(a, b)
})()

// Tuples: destructuring
(() => {
  val (a, b, c) = (1, "two", 3)
  println(a, b, c)

  val (d, (e, f), (g, h, i)) = (1, (2, 3), (4, 5, 6))
  println(d, e, f, g, h, i)
})()

