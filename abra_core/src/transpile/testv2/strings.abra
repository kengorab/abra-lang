val s = "hello"

/// Expect: h e l l o
println(s[0], s[1], s[2], s[3], s[4])

/// Expect: o l l e h
println(s[-1], s[-2], s[-3], s[-4], s[-5])

/// Expect:   end
println(s[5], s[-6], "end")

/// Expect: ell ell
println(s[1:4], s[-4:4])

/// Expect:  end
println(s[1:1], "end")

val a = 1
val b = 4

/// Expect: ell ell
println(s[a:b], s[-b:b])

/// Expect: ello h
println(s[1:], s[:1])

/// Expect: ello h
println(s[a:], s[:a])

// Fields
/// Expect: 5
println(s.length)
