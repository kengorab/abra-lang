val TOP_LEVEL_STRING = "hello"
val TOP_LEVEL_INT = 123

val f1 = () => println(TOP_LEVEL_STRING, TOP_LEVEL_INT)
/// Expect: hello 123
f1()

func outer(): (Int) => Int {
  val a = 1
  val inner = (b: Int) => a + b
  inner
}
/// Expect: 3 3
val f = outer()
println(f(2), outer()(2))

var A = "foo"
func f2() { A += "!" }
/// Expect: foo
println(A)
f2()
/// Expect: foo!
println(A)

A = "hello"
f2()
/// Expect: hello!
println(A)
