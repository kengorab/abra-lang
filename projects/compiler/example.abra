(() => {
  var capturedFloat = 1.1
  val closure1 = (one: Int) => {
    capturedFloat += 1.1
    val x = capturedFloat + one
    x
  }
  /// Expect: 1.1
  stdoutWriteln(capturedFloat.toString())
  /// Expect: 3.2
  stdoutWriteln(closure1(1).toString())
  /// Expect: 2.2
  stdoutWriteln(capturedFloat.toString())
})()