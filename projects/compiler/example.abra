enum Foo {
  Foo
  Bar//(a: String)
  Baz
}

type Bar {
  foo: Foo

  func getFoo(self): Foo = self.foo
}

val b: Bar? = None //Some(Bar(foo: Foo.Foo))
if b?.getFoo() |f| {
  println(f)
} else {
  println("nope")
}

// val f: Foo? = None // Some(Foo.Foo)

match b?.getFoo() {
  None => println("none")
  Foo.Bar => println("Foo.Bar")
  Foo.Baz => println("Foo.Baz")
  else => println("Foo.Foo")
}
