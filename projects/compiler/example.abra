type Adder {
  i: Int

  func add(self, x: Int): Int = self.i + x
}

func addA(x: Int, y = 1): Int = x + y
func addB(x: Int, y = 1): Int = x + y
func addC(x: Float, y = 1.1): Float = x + y

func f() {
  val add1 = addA
  val add2 = addB
  val add3 = addC

  println(add1(3))
  println(add2(3))
  println(add3(3.14))

  val adder = Adder(i: 1)
  val doAdd = adder.add
  println(doAdd(11))
}
f()
