import Token, TokenKind, StringInterpolationChunk from "./lexer"
import Label, ImportKind, ParsedModule, AstNode, AstNodeKind, LiteralAstNode, IdentifierKind, IndexingMode, TypeIdentifier, AssignmentMode, BindingPattern, MatchCaseKind, DecoratorNode, EnumVariant, ForIterKind from "./parser"

pub func printTokenAsJson(token: Token, indentLevelStart: Int, currentIndentLevel: Int) {
  val startIndent = "  ".repeat(indentLevelStart)
  val fieldsIndent = "  ".repeat(currentIndentLevel + 1)
  val endIndent = "  ".repeat(currentIndentLevel)

  stdoutWriteln("$startIndent{\n$fieldsIndent\"position\": [${token.position.line}, ${token.position.col}],")
  stdoutWrite("$fieldsIndent\"kind\": ")
  printTokenKindAsJson(kind: token.kind, indentLevelStart: 0, currentIndentLevel: currentIndentLevel + 1)
  stdoutWrite("$endIndent}")
}

func printTokenKindAsJson(kind: TokenKind, indentLevelStart: Int, currentIndentLevel: Int) {
  val startIndent = "  ".repeat(indentLevelStart)
  val fieldsIndent = "  ".repeat(currentIndentLevel + 1)
  val endIndent = "  ".repeat(currentIndentLevel)

  stdoutWriteln("$startIndent{")
  match kind {
    TokenKind.Int(value) => {
      stdoutWriteln("$fieldsIndent\"name\": \"Int\",")
      stdoutWriteln("$fieldsIndent\"value\": $value")
    }
    TokenKind.Float(value) => {
      stdoutWriteln("$fieldsIndent\"name\": \"Float\",")
      stdoutWriteln("$fieldsIndent\"value\": $value")
    }
    TokenKind.Bool(value) => {
      stdoutWriteln("$fieldsIndent\"name\": \"Bool\",")
      stdoutWriteln("$fieldsIndent\"value\": $value")
    }
    TokenKind.Char(intVal) => {
      stdoutWriteln("$fieldsIndent\"name\": \"Char\",")
      stdoutWriteln("$fieldsIndent\"value\": \"${intVal.hex()}\"")
    }
    TokenKind.String(value) => {
      stdoutWriteln("$fieldsIndent\"name\": \"String\",")
      stdoutWriteln("$fieldsIndent\"value\": \"$value\"")
    }
    TokenKind.StringInterpolation(chunks) => {
      stdoutWriteln("$fieldsIndent\"name\": \"StringInterpolation\",")
      stdoutWriteln("$fieldsIndent\"chunks\": [")
      for chunk, idx in chunks {
        match chunk {
          StringInterpolationChunk.String(pos, value) => {
            printTokenAsJson(Token(position: pos, kind: TokenKind.String(value)), indentLevelStart + currentIndentLevel + 2, currentIndentLevel + 2)
          }
          StringInterpolationChunk.Interpolation(tokens) => {
            stdoutWriteln("$fieldsIndent  [")
            for token, idx in tokens {
              printTokenAsJson(token, indentLevelStart + currentIndentLevel + 3, currentIndentLevel + 3)

              val comma = if idx != tokens.length - 1 "," else ""
              stdoutWriteln(comma)
            }
            stdoutWrite("$fieldsIndent  ]")
          }
        }
        val comma = if idx != chunks.length - 1 "," else ""
        stdoutWriteln(comma)
      }
      stdoutWriteln("$fieldsIndent]")
    }
    TokenKind.Ident(name) => {
      stdoutWriteln("$fieldsIndent\"name\": \"Ident\",")
      stdoutWriteln("$fieldsIndent\"value\": \"$name\"")
    }
    TokenKind.If => stdoutWriteln("$fieldsIndent\"name\": \"If\"")
    TokenKind.Else => stdoutWriteln("$fieldsIndent\"name\": \"Else\"")
    TokenKind.Val => stdoutWriteln("$fieldsIndent\"name\": \"Val\"")
    TokenKind.Var => stdoutWriteln("$fieldsIndent\"name\": \"Var\"")
    TokenKind.Func => stdoutWriteln("$fieldsIndent\"name\": \"Func\"")
    TokenKind.Self => stdoutWriteln("$fieldsIndent\"name\": \"Self\"")
    TokenKind.While => stdoutWriteln("$fieldsIndent\"name\": \"While\"")
    TokenKind.Break => stdoutWriteln("$fieldsIndent\"name\": \"Break\"")
    TokenKind.Continue => stdoutWriteln("$fieldsIndent\"name\": \"Continue\"")
    TokenKind.For => stdoutWriteln("$fieldsIndent\"name\": \"For\"")
    TokenKind.In => stdoutWriteln("$fieldsIndent\"name\": \"In\"")
    TokenKind.Match => stdoutWriteln("$fieldsIndent\"name\": \"Match\"")
    TokenKind.Type => stdoutWriteln("$fieldsIndent\"name\": \"Type\"")
    TokenKind.Enum => stdoutWriteln("$fieldsIndent\"name\": \"Enum\"")
    TokenKind.Decorator => stdoutWriteln("$fieldsIndent\"name\": \"Decorator\"")
    TokenKind.Return => stdoutWriteln("$fieldsIndent\"name\": \"Return\"")
    TokenKind.Readonly => stdoutWriteln("$fieldsIndent\"name\": \"Readonly\"")
    TokenKind.Import => stdoutWriteln("$fieldsIndent\"name\": \"Import\"")
    TokenKind.From => stdoutWriteln("$fieldsIndent\"name\": \"From\"")
    TokenKind.As => stdoutWriteln("$fieldsIndent\"name\": \"As\"")
    TokenKind.Try => stdoutWriteln("$fieldsIndent\"name\": \"Try\"")
    TokenKind.None_ => stdoutWriteln("$fieldsIndent\"name\": \"None\"")
    TokenKind.Pub => stdoutWriteln("$fieldsIndent\"name\": \"Pub\"")
    TokenKind.Plus => stdoutWriteln("$fieldsIndent\"name\": \"Plus\"")
    TokenKind.PlusEq => stdoutWriteln("$fieldsIndent\"name\": \"PlusEq\"")
    TokenKind.Minus => stdoutWriteln("$fieldsIndent\"name\": \"Minus\"")
    TokenKind.MinusEq => stdoutWriteln("$fieldsIndent\"name\": \"MinusEq\"")
    TokenKind.Star => stdoutWriteln("$fieldsIndent\"name\": \"Star\"")
    TokenKind.StarEq => stdoutWriteln("$fieldsIndent\"name\": \"StarEq\"")
    TokenKind.StarStar => stdoutWriteln("$fieldsIndent\"name\": \"StarStar\"")
    TokenKind.Slash => stdoutWriteln("$fieldsIndent\"name\": \"Slash\"")
    TokenKind.SlashEq => stdoutWriteln("$fieldsIndent\"name\": \"SlashEq\"")
    TokenKind.Percent => stdoutWriteln("$fieldsIndent\"name\": \"Percent\"")
    TokenKind.PercentEq => stdoutWriteln("$fieldsIndent\"name\": \"PercentEq\"")
    TokenKind.LT => stdoutWriteln("$fieldsIndent\"name\": \"LT\"")
    TokenKind.LTE => stdoutWriteln("$fieldsIndent\"name\": \"LTE\"")
    TokenKind.GT => stdoutWriteln("$fieldsIndent\"name\": \"GT\"")
    TokenKind.GTE => stdoutWriteln("$fieldsIndent\"name\": \"GTE\"")
    TokenKind.Bang => stdoutWriteln("$fieldsIndent\"name\": \"Bang\"")
    TokenKind.Eq => stdoutWriteln("$fieldsIndent\"name\": \"Assign\"")
    TokenKind.EqEq => stdoutWriteln("$fieldsIndent\"name\": \"Eq\"")
    TokenKind.Neq => stdoutWriteln("$fieldsIndent\"name\": \"Neq\"")
    TokenKind.Arrow => stdoutWriteln("$fieldsIndent\"name\": \"Arrow\"")
    TokenKind.Dot => stdoutWriteln("$fieldsIndent\"name\": \"Dot\"")
    TokenKind.Or => stdoutWriteln("$fieldsIndent\"name\": \"Or\"")
    TokenKind.OrEq => stdoutWriteln("$fieldsIndent\"name\": \"OrEq\"")
    TokenKind.And => stdoutWriteln("$fieldsIndent\"name\": \"And\"")
    TokenKind.AndEq => stdoutWriteln("$fieldsIndent\"name\": \"AndEq\"")
    TokenKind.Caret => stdoutWriteln("$fieldsIndent\"name\": \"Caret\"")
    TokenKind.LParen => stdoutWriteln("$fieldsIndent\"name\": \"LParen\"")
    TokenKind.RParen => stdoutWriteln("$fieldsIndent\"name\": \"RParen\"")
    TokenKind.LBrack => stdoutWriteln("$fieldsIndent\"name\": \"LBrack\"")
    TokenKind.RBrack => stdoutWriteln("$fieldsIndent\"name\": \"RBrack\"")
    TokenKind.LBrace => stdoutWriteln("$fieldsIndent\"name\": \"LBrace\"")
    TokenKind.RBrace => stdoutWriteln("$fieldsIndent\"name\": \"RBrace\"")
    TokenKind.Pipe => stdoutWriteln("$fieldsIndent\"name\": \"Pipe\"")
    TokenKind.Comma => stdoutWriteln("$fieldsIndent\"name\": \"Comma\"")
    TokenKind.Colon => stdoutWriteln("$fieldsIndent\"name\": \"Colon\"")
    TokenKind.Question => stdoutWriteln("$fieldsIndent\"name\": \"Question\"")
    TokenKind.Elvis => stdoutWriteln("$fieldsIndent\"name\": \"Elvis\"")
    TokenKind.ElvisEq => stdoutWriteln("$fieldsIndent\"name\": \"ElvisEq\"")
    TokenKind.QuestionDot => stdoutWriteln("$fieldsIndent\"name\": \"QuestionDot\"")
    TokenKind.HashBrace => stdoutWriteln("$fieldsIndent\"name\": \"LBraceHash\"")
    TokenKind.At => stdoutWriteln("$fieldsIndent\"name\": \"At\"")
  }
  stdoutWriteln("$endIndent}")
}

pub func printParsedModuleAsJson(module: ParsedModule) {
  stdoutWriteln("{")
  if module.imports.isEmpty() {
    stdoutWriteln("  \"imports\": [],")
  } else {
    stdoutWriteln("  \"imports\": [")
    for importNode, idx in module.imports {
      stdoutWriteln("    {")
      stdoutWrite("      \"token\": ")
      printTokenAsJson(importNode.token, 0, 3)
      stdoutWrite(",\n      \"moduleName\": ")
      printLabelAsJson(importNode.moduleName)
      stdoutWriteln(",\n      \"importKind\": {")
      match importNode.kind {
        ImportKind.Alias(alias) => {
          stdoutWriteln("        \"kind\": \"alias\",")
          stdoutWrite("        \"alias\": ")
          printLabelAsJson(alias)
          stdoutWriteln()
        }
        ImportKind.List(names) => {
          stdoutWriteln("        \"kind\": \"list\",")
          stdoutWriteln("        \"names\": [")
          for name, idx in names {
            stdoutWrite("          ")
            printLabelAsJson(name)
            val comma = if idx != names.length - 1 "," else ""
            stdoutWriteln(comma)
          }
          stdoutWriteln("        ]")
        }
      }
      stdoutWriteln("      }")
      val comma = if idx != module.imports.length - 1 "," else ""
      stdoutWriteln("    }$comma")
    }
    stdoutWriteln("  ],")
  }
  if module.nodes.isEmpty() {
    stdoutWriteln("  \"nodes\": []")
  } else {
    stdoutWriteln("  \"nodes\": [")
    for node, idx in module.nodes {
      printAstNodeAsJson(node, 2, 2)
      val comma = if idx != module.nodes.length - 1 "," else ""
      stdoutWriteln(comma)
    }
    stdoutWriteln("  ]")
  }
  stdoutWriteln("}")
}

pub func printLabelAsJson(label: Label) {
  stdoutWrite("{ \"name\": \"${label.name}\", \"position\": [${label.position.line}, ${label.position.col}] }")
}

func printTypeIdentifierAsJson(typeIdentifier: TypeIdentifier, indentLevelStart: Int, currentIndentLevel: Int) {
  val startIndent = "  ".repeat(indentLevelStart)
  val fieldsIndent = "  ".repeat(currentIndentLevel + 1)
  val endIndent = "  ".repeat(currentIndentLevel)

  stdoutWrite("$startIndent{\n$fieldsIndent\"kind\": ")
  match typeIdentifier {
    TypeIdentifier.Normal(label, typeArguments, path) => {
      stdoutWriteln("\"normal\",")
      if !path.isEmpty() {
        stdoutWriteln("$fieldsIndent\"path\": [")
        for seg, idx in path {
          stdoutWrite("$fieldsIndent  ")
          printLabelAsJson(seg)
          val comma = if idx != path.length - 1 "," else ""
          stdoutWriteln(comma)
        }
        stdoutWriteln("$fieldsIndent],")
      }
      stdoutWrite("$fieldsIndent\"label\": ")
      printLabelAsJson(label)
      if typeArguments.isEmpty() {
        stdoutWriteln(",\n$fieldsIndent\"typeArguments\": []")
      } else {
        stdoutWriteln(",\n$fieldsIndent\"typeArguments\": [")
        for arg, idx in typeArguments {
          printTypeIdentifierAsJson(arg, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != typeArguments.length - 1 "," else ""
          stdoutWriteln(comma)
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    TypeIdentifier.Array(inner) => {
      stdoutWrite("\"array\",\n$fieldsIndent\"inner\": ")
      printTypeIdentifierAsJson(inner, 0, currentIndentLevel + 1)
      stdoutWriteln()
    }
    TypeIdentifier.Option(inner) => {
      stdoutWrite("\"option\",\n$fieldsIndent\"inner\": ")
      printTypeIdentifierAsJson(inner, 0, currentIndentLevel + 1)
      stdoutWriteln()
    }
    TypeIdentifier.Tuple(typeArguments) => {
      stdoutWriteln("\"tuple\",\n$fieldsIndent\"typeArguments\": [")
      for arg, idx in typeArguments {
        printTypeIdentifierAsJson(arg, currentIndentLevel + 2, currentIndentLevel + 2)
        val comma = if idx != typeArguments.length - 1 "," else ""
        stdoutWriteln(comma)
      }
      stdoutWriteln("$fieldsIndent]")
    }
    TypeIdentifier.Function(argTypes, retType) => {
      stdoutWriteln("\"function\",")
      if argTypes.isEmpty() {
        stdoutWriteln("$fieldsIndent\"argumentTypes\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"argumentTypes\": [")
        for arg, idx in argTypes {
          printTypeIdentifierAsJson(arg, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != argTypes.length - 1 "," else ""
          stdoutWriteln(comma)
        }
        stdoutWrite("$fieldsIndent],\n")
      }
      stdoutWrite("$fieldsIndent\"returnType\": ")
      printTypeIdentifierAsJson(retType, 0, currentIndentLevel + 1)
      stdoutWriteln()
    }
  }
  stdoutWrite("$endIndent}")
}

func printDecoratorNodeAsJson(dec: DecoratorNode, indentLevelStart: Int, currentIndentLevel: Int) {
  val startIndent = "  ".repeat(indentLevelStart)
  val fieldsIndent = "  ".repeat(currentIndentLevel + 1)
  val endIndent = "  ".repeat(currentIndentLevel)

  stdoutWrite("$startIndent{\n$fieldsIndent\"name\": ")
  printLabelAsJson(dec.name)

  if dec.arguments.isEmpty() {
    stdoutWriteln(",\n$fieldsIndent\"arguments\": []")
  } else {
    stdoutWriteln(",\n$fieldsIndent\"arguments\": [")
    val pathIndent = "  ".repeat(currentIndentLevel + 2)
    val pathsIndent = "  ".repeat(currentIndentLevel + 3)
    for arg, idx in dec.arguments {
      stdoutWrite("$pathIndent{\n$pathsIndent\"label\": ")
      if arg.label |label| {
        printLabelAsJson(label)
        stdoutWriteln(",")
      } else {
        stdoutWriteln("null,")
      }
      stdoutWrite("$pathsIndent\"value\": ")
      printAstNodeAsJson(arg.value, 0, currentIndentLevel + 3)
      val comma = if idx != dec.arguments.length - 1 "," else ""
      stdoutWriteln("\n$pathIndent}$comma")
    }
    stdoutWriteln("$fieldsIndent]")
  }

  stdoutWrite("$endIndent}")
}

pub func printBindingPatternAsJson(bindingPattern: BindingPattern, indentLevelStart: Int, currentIndentLevel: Int) {
  val startIndent = "  ".repeat(indentLevelStart)
  val fieldsIndent = "  ".repeat(currentIndentLevel + 1)
  val endIndent = "  ".repeat(currentIndentLevel)

  stdoutWrite("$startIndent{\n$fieldsIndent\"kind\": ")
  match bindingPattern {
    BindingPattern.Variable(label) => {
      stdoutWrite("\"variable\",\n$fieldsIndent\"label\": ")
      printLabelAsJson(label)
    }
    BindingPattern.Tuple(_, patterns) => {
      stdoutWriteln("\"tuple\",\n$fieldsIndent\"patterns\": [")
      for pat, idx in patterns {
        printBindingPatternAsJson(pat, currentIndentLevel + 2, currentIndentLevel + 2)
        if idx != patterns.length - 1 {
          stdoutWriteln(",")
        }
      }
      stdoutWrite("\n$fieldsIndent]")
    }
  }
  stdoutWrite("\n$endIndent}")
}

func printAstNodeAsJson(node: AstNode, indentLevelStart: Int, currentIndentLevel: Int) {
  val startIndent = "  ".repeat(indentLevelStart)
  val fieldsIndent = "  ".repeat(currentIndentLevel + 1)
  val endIndent = "  ".repeat(currentIndentLevel)

  stdoutWrite("$startIndent{\n$fieldsIndent\"token\": ")
  printTokenAsJson(token: node.token, indentLevelStart: 0, currentIndentLevel: currentIndentLevel + 1)
  stdoutWrite(",\n$fieldsIndent\"kind\": ")
  printAstNodeKindAsJson(kind: node.kind, indentLevelStart: 0, currentIndentLevel: currentIndentLevel + 1)
  stdoutWrite("\n$endIndent}")
}

func printAstNodeKindAsJson(kind: AstNodeKind, indentLevelStart: Int, currentIndentLevel: Int) {
  val startIndent = "  ".repeat(indentLevelStart)
  val fieldsIndent = "  ".repeat(currentIndentLevel + 1)
  val endIndent = "  ".repeat(currentIndentLevel)

  stdoutWriteln("$startIndent{")
  match kind {
    AstNodeKind.Literal(literal) => {
      val (kind, value) = match literal {
        LiteralAstNode.Int(value) => ("int", value.toString())
        LiteralAstNode.Float(value) => ("float", value.toString())
        LiteralAstNode.Bool(value) => ("bool", value.toString())
        LiteralAstNode.Char(value) => {
          ("char", "\"${value.hex()}\"")
        }
        LiteralAstNode.String(value) => ("string", "\"$value\"")
      }

      stdoutWriteln("$fieldsIndent\"name\": \"literal\",")
      stdoutWriteln("$fieldsIndent\"type\": \"$kind\",")
      stdoutWriteln("$fieldsIndent\"value\": $value")
    }
    AstNodeKind.StringInterpolation(exprs) => {
      stdoutWriteln("$fieldsIndent\"name\": \"stringInterpolation\",")
      if exprs.isEmpty() {
        stdoutWriteln("$fieldsIndent\"exprs\": []")
      } else {
        stdoutWriteln("$fieldsIndent\"exprs\": [")
        for item, idx in exprs {
          printAstNodeAsJson(item, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != exprs.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.Unary(unaryNode) => {
      stdoutWriteln("$fieldsIndent\"name\": \"unary\",")
      stdoutWriteln("$fieldsIndent\"op\": \"${unaryNode.op}\",")
      stdoutWrite("$fieldsIndent\"expr\": ")
      printAstNodeAsJson(unaryNode.expr, 0, currentIndentLevel + 1)
      stdoutWriteln()
    }
    AstNodeKind.Binary(binaryNode) => {
      stdoutWriteln("$fieldsIndent\"name\": \"binary\",")
      stdoutWriteln("$fieldsIndent\"op\": \"${binaryNode.op}\",")
      stdoutWrite("$fieldsIndent\"left\": ")
      printAstNodeAsJson(binaryNode.left, 0, currentIndentLevel + 1)
      stdoutWrite(",\n$fieldsIndent\"right\": ")
      printAstNodeAsJson(binaryNode.right, 0, currentIndentLevel + 1)
      stdoutWriteln()
    }
    AstNodeKind.Grouped(inner) => {
      stdoutWriteln("$fieldsIndent\"name\": \"grouped\",")
      stdoutWrite("$fieldsIndent\"inner\": ")
      printAstNodeAsJson(inner, 0, currentIndentLevel + 1)
      stdoutWriteln()
    }
    AstNodeKind.Identifier(kind) => {
      stdoutWriteln("$fieldsIndent\"name\": \"identifier\",")
      stdoutWrite("$fieldsIndent\"ident\": ")
      match kind {
        IdentifierKind.Named(name) => stdoutWriteln("\"$name\"")
        IdentifierKind.None_ => stdoutWriteln("\"None\"")
        IdentifierKind.Self => stdoutWriteln("\"self\"")
        IdentifierKind.Discard => stdoutWriteln("\"_\"")
      }
    }
    AstNodeKind.Accessor(accessorNode) => {
      stdoutWriteln("$fieldsIndent\"name\": \"accessor\",")
      stdoutWrite("$fieldsIndent\"root\": ")
      printAstNodeAsJson(accessorNode.root, 0, currentIndentLevel + 1)
      stdoutWriteln(",\n$fieldsIndent\"path\": [")
      val pathIndent = "  ".repeat(currentIndentLevel + 2)
      val pathsIndent = "  ".repeat(currentIndentLevel + 3)
      for (dotToken, label), idx in accessorNode.path {
        stdoutWrite("$pathIndent{\n$pathsIndent\"dotToken\": ")
        printTokenAsJson(dotToken, 0, currentIndentLevel + 3)
        stdoutWriteln(",\n$pathsIndent\"ident\": \"${label.name}\",")
        stdoutWriteln("$pathsIndent\"position\": [${label.position.line}, ${label.position.col}]")
        val comma = if idx != accessorNode.path.length - 1 "," else ""
        stdoutWriteln("$pathIndent}$comma")
      }
      stdoutWriteln("$fieldsIndent]")
    }
    AstNodeKind.Invocation(invocationNode) => {
      stdoutWriteln("$fieldsIndent\"name\": \"invocation\",")
      stdoutWrite("$fieldsIndent\"invokee\": ")
      printAstNodeAsJson(invocationNode.invokee, 0, currentIndentLevel + 1)
      if invocationNode.typeArguments.isEmpty() {
        stdoutWriteln(",\n$fieldsIndent\"typeArguments\": [],")
      } else {
        stdoutWriteln(",\n$fieldsIndent\"typeArguments\": [")
        for typeArg, idx in invocationNode.typeArguments {
          printTypeIdentifierAsJson(typeArg, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != invocationNode.typeArguments.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }

      if invocationNode.arguments.isEmpty() {
        stdoutWriteln("$fieldsIndent\"arguments\": []")
      } else {
        stdoutWriteln("$fieldsIndent\"arguments\": [")
        val pathIndent = "  ".repeat(currentIndentLevel + 2)
        val pathsIndent = "  ".repeat(currentIndentLevel + 3)
        for arg, idx in invocationNode.arguments {
          stdoutWrite("$pathIndent{\n$pathsIndent\"label\": ")
          if arg.label |label| {
            printLabelAsJson(label)
            stdoutWriteln(",")
          } else {
            stdoutWriteln("null,")
          }
          stdoutWrite("$pathsIndent\"value\": ")
          printAstNodeAsJson(arg.value, 0, currentIndentLevel + 3)
          val comma = if idx != invocationNode.arguments.length - 1 "," else ""
          stdoutWriteln("\n$pathIndent}$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.Array(items) => {
      stdoutWriteln("$fieldsIndent\"name\": \"array\",")
      if items.isEmpty() {
        stdoutWriteln("$fieldsIndent\"items\": []")
      } else {
        stdoutWriteln("$fieldsIndent\"items\": [")
        for item, idx in items {
          printAstNodeAsJson(item, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != items.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.Set(items) => {
      stdoutWriteln("$fieldsIndent\"name\": \"set\",")
      if items.isEmpty() {
        stdoutWriteln("$fieldsIndent\"items\": []")
      } else {
        stdoutWriteln("$fieldsIndent\"items\": [")
        for item, idx in items {
          printAstNodeAsJson(item, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != items.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.Map(items) => {
      stdoutWriteln("$fieldsIndent\"name\": \"map\",")
      if items.isEmpty() {
        stdoutWriteln("$fieldsIndent\"items\": []")
      } else {
        stdoutWriteln("$fieldsIndent\"items\": [")
        for (key, value), idx in items {
          stdoutWriteln("$fieldsIndent  {")
          stdoutWrite("$fieldsIndent    \"key\": ")
          printAstNodeAsJson(key, 0, currentIndentLevel + 3)
          stdoutWrite(",\n$fieldsIndent    \"value\": ")
          printAstNodeAsJson(value, 0, currentIndentLevel + 3)
          val comma = if idx != items.length - 1 "," else ""
          stdoutWriteln("\n$fieldsIndent  }$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.Tuple(items) => {
      stdoutWriteln("$fieldsIndent\"name\": \"tuple\",")
      if items.isEmpty() {
        stdoutWriteln("$fieldsIndent\"items\": []")
      } else {
        stdoutWriteln("$fieldsIndent\"items\": [")
        for item, idx in items {
          printAstNodeAsJson(item, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != items.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.Indexing(expr, indexMode) => {
      stdoutWriteln("$fieldsIndent\"name\": \"indexing\",")
      stdoutWrite("$fieldsIndent\"expr\": ")
      printAstNodeAsJson(expr, 0, currentIndentLevel + 1)
      stdoutWriteln(",\n$fieldsIndent\"index\": {")
      match indexMode {
        IndexingMode.Single(item) => {
          stdoutWriteln("$fieldsIndent  \"indexKind\": \"single\",")
          stdoutWrite("$fieldsIndent  \"value\": ")
          printAstNodeAsJson(item, 0, currentIndentLevel + 2)
        }
        IndexingMode.Range(start, end) => {
          stdoutWriteln("$fieldsIndent  \"indexKind\": \"range\",")
          if start |start| {
            stdoutWrite("$fieldsIndent  \"start\": ")
            printAstNodeAsJson(start, 0, currentIndentLevel + 2)
            stdoutWriteln(",")
          } else {
            stdoutWriteln("$fieldsIndent  \"start\": null,")
          }
          if end |end| {
            stdoutWrite("$fieldsIndent  \"end\": ")
            printAstNodeAsJson(end, 0, currentIndentLevel + 2)
          } else {
            stdoutWrite("$fieldsIndent  \"end\": null")
          }
        }
      }
      stdoutWriteln("\n$fieldsIndent}")
    }
    AstNodeKind.Lambda(node) => {
      stdoutWriteln("$fieldsIndent\"name\": \"lambda\",")
      if node.params.isEmpty() {
        stdoutWriteln("$fieldsIndent\"params\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"params\": [")
        for p, idx in node.params {
          stdoutWriteln("$fieldsIndent  {")
          stdoutWrite("$fieldsIndent    \"label\": ")
          printLabelAsJson(p.label)
          stdoutWriteln(",\n$fieldsIndent    \"isVariadic\": ${p.isVariadic},")
          stdoutWrite("$fieldsIndent    \"typeAnnotation\": ")
          if p.typeAnnotation |ann| {
            printTypeIdentifierAsJson(ann, 0, currentIndentLevel + 3)
          } else {
            stdoutWrite("null")
          }
          stdoutWrite(",\n$fieldsIndent    \"defaultValue\": ")
          if p.defaultValue |defaultValue| {
            printAstNodeAsJson(defaultValue, 0, currentIndentLevel + 3)
          } else {
            stdoutWrite("null")
          }
          val comma = if idx != node.params.length - 1 "," else ""
          stdoutWriteln("\n$fieldsIndent  }$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }
      if node.body.isEmpty() {
        stdoutWriteln("$fieldsIndent\"body\": []")
      } else {
        stdoutWriteln("$fieldsIndent\"body\": [")
        for n, idx in node.body {
          printAstNodeAsJson(n, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != node.body.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.Assignment(expr, op, mode) => {
      stdoutWriteln("$fieldsIndent\"name\": \"assignment\",")
      stdoutWrite("$fieldsIndent\"expr\": ")
      printAstNodeAsJson(expr, 0, currentIndentLevel + 1)
      stdoutWriteln(",\n$fieldsIndent\"op\": \"$op\",")
      stdoutWrite("$fieldsIndent\"mode\": ")
      match mode {
        AssignmentMode.Variable(name, _) => {
          stdoutWriteln("{\n$fieldsIndent  \"mode\": \"variable\",")
          stdoutWriteln("$fieldsIndent  \"variable\": \"$name\"")
          stdoutWriteln("$fieldsIndent}")
        }
        AssignmentMode.Indexing(expr, indexExpr) => {
          stdoutWriteln("{\n$fieldsIndent  \"mode\": \"indexing\",")
          stdoutWrite("$fieldsIndent  \"expr\": ")
          printAstNodeAsJson(expr, 0, currentIndentLevel + 2)
          stdoutWriteln(",")
          stdoutWrite("$fieldsIndent  \"indexExpr\": ")
          printAstNodeAsJson(indexExpr, 0, currentIndentLevel + 2)
          stdoutWriteln("\n$fieldsIndent}")
        }
        AssignmentMode.Accessor(node) => {
          printAstNodeKindAsJson(AstNodeKind.Accessor(node), 0, currentIndentLevel + 1)
          stdoutWriteln()
        }
      }
    }
    AstNodeKind.If(condition, conditionBinding, ifBlock, elseBlock) => {
      stdoutWriteln("$fieldsIndent\"name\": \"if\",")
      stdoutWrite("$fieldsIndent\"condition\": ")
      printAstNodeAsJson(condition, 0, currentIndentLevel + 1)
      if conditionBinding |pat| {
        stdoutWrite(",\n$fieldsIndent\"conditionBinding\": ")
        printBindingPatternAsJson(pat, 0, currentIndentLevel + 1)
      } else {
        stdoutWrite(",\n$fieldsIndent\"conditionBinding\": null")
      }
      if ifBlock.isEmpty() {
        stdoutWriteln(",\n$fieldsIndent\"ifBlock\": [],")
      } else {
        stdoutWriteln(",\n$fieldsIndent\"ifBlock\": [")
        for node, idx in ifBlock {
          printAstNodeAsJson(node, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != ifBlock.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }
      if elseBlock |block| {
        if block.isEmpty() {
          stdoutWriteln("$fieldsIndent\"elseBlock\": []")
        } else {
          stdoutWriteln("$fieldsIndent\"elseBlock\": [")
          for node, idx in block {
            printAstNodeAsJson(node, currentIndentLevel + 2, currentIndentLevel + 2)
            val comma = if idx != block.length - 1 "," else ""
            stdoutWriteln("$comma")
          }
          stdoutWriteln("$fieldsIndent]")
        }
      } else {
        stdoutWriteln("$fieldsIndent\"elseBlock\": null")
      }
    }
    AstNodeKind.Match(expr, cases) => {
      stdoutWriteln("$fieldsIndent\"name\": \"match\",")
      stdoutWrite("$fieldsIndent\"expr\": ")
      printAstNodeAsJson(expr, 0, currentIndentLevel + 1)
      if cases.isEmpty() {
        stdoutWriteln(",\n$fieldsIndent\"cases\": [],")
      } else {
        stdoutWriteln(",\n$fieldsIndent\"cases\": [")
        for case, idx in cases {
          stdoutWriteln("$fieldsIndent  {")
          stdoutWriteln("$fieldsIndent    \"caseKind\": {")
          match case.kind {
            MatchCaseKind.None_ => {
              stdoutWriteln("$fieldsIndent      \"kind\": \"none\"")
            }
            MatchCaseKind.Literal(literal) => {
              stdoutWriteln("$fieldsIndent      \"kind\": \"literal\",")
              val (kind, value) = match literal {
                LiteralAstNode.Int(value) => ("int", value.toString())
                LiteralAstNode.Float(value) => ("float", value.toString())
                LiteralAstNode.Bool(value) => ("bool", value.toString())
                LiteralAstNode.Char(value) => {
                  ("char", "\"${value.hex()}\"")
                }
                LiteralAstNode.String(value) => ("string", "\"$value\"")
              }

              stdoutWriteln("$fieldsIndent      \"type\": \"$kind\",")
              stdoutWriteln("$fieldsIndent      \"value\": $value")
            }
            MatchCaseKind.Type(path, last, args) => {
              stdoutWriteln("$fieldsIndent      \"kind\": \"type\",")
              stdoutWriteln("$fieldsIndent      \"path\": [")
              val fullPath = path.concat([last])
              for label, idx in fullPath {
                stdoutWrite("$fieldsIndent        ")
                printLabelAsJson(label)
                val comma = if idx != fullPath.length - 1 "," else ""
                stdoutWriteln("$comma")
              }
              stdoutWriteln("$fieldsIndent      ],")
              stdoutWriteln("$fieldsIndent      \"arguments\": [")
              for label, idx in args {
                stdoutWrite("$fieldsIndent        ")
                printLabelAsJson(label)
                val comma = if idx != args.length - 1 "," else ""
                stdoutWriteln("$comma")
              }
              stdoutWriteln("$fieldsIndent      ]")
            }
            MatchCaseKind.Else => {
              stdoutWriteln("$fieldsIndent      \"kind\": \"else\"")
            }
          }
          stdoutWriteln("$fieldsIndent    },")
          if case.binding |binding| {
            stdoutWrite("$fieldsIndent    \"caseBinding\": ")
            printLabelAsJson(binding)
            stdoutWriteln(",")
          } else {
            stdoutWriteln("$fieldsIndent    \"caseBinding\": null,")
          }
          if case.body.isEmpty() {
            stdoutWriteln("$fieldsIndent    \"body\": []")
          } else {
            stdoutWriteln("$fieldsIndent    \"body\": [")
            for node, idx in case.body {
              printAstNodeAsJson(node, currentIndentLevel + 4, currentIndentLevel + 4)
              val comma = if idx != case.body.length - 1 "," else ""
              stdoutWriteln("$comma")
            }
            stdoutWriteln("$fieldsIndent    ]")
          }
          val comma = if idx != cases.length - 1 "," else ""
          stdoutWriteln("$fieldsIndent  }$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.Try(expr, elseClause) => {
      stdoutWriteln("$fieldsIndent\"name\": \"try\",")
      stdoutWrite("$fieldsIndent\"expr\": ")
      printAstNodeAsJson(expr, 0, currentIndentLevel + 1)
      if elseClause |(_, bindingPattern, block)| {
        if bindingPattern |pat| {
          stdoutWrite(",\n$fieldsIndent\"elseBindingPattern\": ")
          printBindingPatternAsJson(pat, 0, currentIndentLevel + 1)
        } else {
          stdoutWrite(",\n$fieldsIndent\"elseBindingPattern\": null")
        }

        if block.isEmpty() {
          stdoutWriteln(",\n$fieldsIndent\"elseBlock\": []")
        } else {
          stdoutWriteln(",\n$fieldsIndent\"elseBlock\": [")
          for node, idx in block {
            printAstNodeAsJson(node, currentIndentLevel + 2, currentIndentLevel + 2)
            val comma = if idx != block.length - 1 "," else ""
            stdoutWriteln("$comma")
          }
          stdoutWrite("$fieldsIndent]")
        }
      }
      stdoutWriteln()
    }
    AstNodeKind.While(condition, conditionBinding, block) => {
      stdoutWriteln("$fieldsIndent\"name\": \"while\",")
      stdoutWrite("$fieldsIndent\"condition\": ")
      printAstNodeAsJson(condition, 0, currentIndentLevel + 1)
      if conditionBinding |pat| {
        stdoutWrite(",\n$fieldsIndent\"conditionBinding\": ")
        printBindingPatternAsJson(pat, 0, currentIndentLevel + 1)
      } else {
        stdoutWrite(",\n$fieldsIndent\"conditionBinding\": null")
      }
      if block.isEmpty() {
        stdoutWriteln(",\n$fieldsIndent\"block\": []")
      } else {
        stdoutWriteln(",\n$fieldsIndent\"block\": [")
        for node, idx in block {
          printAstNodeAsJson(node, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != block.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.For(itemPattern, indexPattern, iterator, block) => {
      stdoutWriteln("$fieldsIndent\"name\": \"for\",")
      stdoutWrite("$fieldsIndent\"itemPattern\": ")
      printBindingPatternAsJson(itemPattern, 0, currentIndentLevel + 1)
      if indexPattern |pat| {
        stdoutWrite(",\n$fieldsIndent\"indexPattern\": ")
        printBindingPatternAsJson(pat, 0, currentIndentLevel + 1)
      } else {
        stdoutWrite(",\n$fieldsIndent\"indexPattern\": null")
      }
      stdoutWrite(",\n$fieldsIndent\"iterator\": ")
      match iterator {
        ForIterKind.Expr(expr) => {
          printAstNodeAsJson(expr, 0, currentIndentLevel + 1)
        }
        ForIterKind.Range(start, end) => {
          stdoutWrite("{\n$fieldsIndent  \"start\": ")
          printAstNodeAsJson(start, 0, currentIndentLevel + 2)
          stdoutWrite(",\n$fieldsIndent  \"end\": ")
          printAstNodeAsJson(end, 0, currentIndentLevel + 2)
          stdoutWrite("\n$fieldsIndent}")
        }
      }
      if block.isEmpty() {
        stdoutWriteln(",\n$fieldsIndent\"block\": []")
      } else {
        stdoutWriteln(",\n$fieldsIndent\"block\": [")
        for node, idx in block {
          printAstNodeAsJson(node, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != block.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.BindingDeclaration(value) => {
      stdoutWriteln("$fieldsIndent\"name\": \"bindingDecl\",")
      if value.decorators.isEmpty() {
        stdoutWriteln("$fieldsIndent\"decorators\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"decorators\": [")
        for dec, idx in value.decorators {
          printDecoratorNodeAsJson(dec, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != value.decorators.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }
      if value.pubToken |token| {
        stdoutWrite("$fieldsIndent\"pubToken\": ")
        printTokenAsJson(token, 0, currentIndentLevel + 1)
        stdoutWriteln(",")
      } else {
        stdoutWriteln("$fieldsIndent\"pubToken\": null,")
      }
      stdoutWrite("$fieldsIndent\"bindingPattern\": ")
      printBindingPatternAsJson(value.bindingPattern, 0, currentIndentLevel + 1)
      stdoutWriteln(",")
      if value.typeAnnotation |ann| {
        stdoutWrite("$fieldsIndent\"typeAnnotation\": ")
        printTypeIdentifierAsJson(ann, 0, currentIndentLevel + 1)
      } else {
        stdoutWrite("$fieldsIndent\"typeAnnotation\": null")
      }
      if value.expr |expr| {
        stdoutWrite(",\n$fieldsIndent\"expr\": ")
        printAstNodeAsJson(expr, 0, currentIndentLevel + 1)
      } else {
        stdoutWrite(",\n$fieldsIndent\"expr\": null")
      }
      stdoutWrite("\n")
    }
    AstNodeKind.FunctionDeclaration(node) => {
      stdoutWriteln("$fieldsIndent\"name\": \"function\",")
      if node.decorators.isEmpty() {
        stdoutWriteln("$fieldsIndent\"decorators\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"decorators\": [")
        for dec, idx in node.decorators {
          printDecoratorNodeAsJson(dec, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != node.decorators.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }
      if node.pubToken |token| {
        stdoutWrite("$fieldsIndent\"pubToken\": ")
        printTokenAsJson(token, 0, currentIndentLevel + 1)
        stdoutWriteln(",")
      } else {
        stdoutWriteln("$fieldsIndent\"pubToken\": null,")
      }
      stdoutWrite("$fieldsIndent\"ident\": ")
      printLabelAsJson(node.name)
      if node.typeParams.isEmpty() {
        stdoutWriteln(",\n$fieldsIndent\"typeParams\": [],")
      } else {
        stdoutWriteln(",\n$fieldsIndent\"typeParams\": [")
        for label, idx in node.typeParams {
          stdoutWrite("$fieldsIndent  ")
          printLabelAsJson(label)
          val comma = if idx != node.typeParams.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }
      if node.params.isEmpty() {
        stdoutWriteln("$fieldsIndent\"params\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"params\": [")
        for p, idx in node.params {
          stdoutWriteln("$fieldsIndent  {")
          stdoutWrite("$fieldsIndent    \"label\": ")
          printLabelAsJson(p.label)
          stdoutWriteln(",\n$fieldsIndent    \"isVariadic\": ${p.isVariadic},")
          stdoutWrite("$fieldsIndent    \"typeAnnotation\": ")
          if p.typeAnnotation |ann| {
            printTypeIdentifierAsJson(ann, 0, currentIndentLevel + 3)
          } else {
            stdoutWrite("null")
          }
          stdoutWrite(",\n$fieldsIndent    \"defaultValue\": ")
          if p.defaultValue |defaultValue| {
            printAstNodeAsJson(defaultValue, 0, currentIndentLevel + 3)
          } else {
            stdoutWrite("null")
          }
          val comma = if idx != node.params.length - 1 "," else ""
          stdoutWriteln("\n$fieldsIndent  }$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }
      if node.body.isEmpty() {
        stdoutWriteln("$fieldsIndent\"body\": []")
      } else {
        stdoutWriteln("$fieldsIndent\"body\": [")
        for n, idx in node.body {
          printAstNodeAsJson(n, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != node.body.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.TypeDeclaration(node) => {
      stdoutWriteln("$fieldsIndent\"name\": \"typeDeclaration\",")
      if node.decorators.isEmpty() {
        stdoutWriteln("$fieldsIndent\"decorators\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"decorators\": [")
        for dec, idx in node.decorators {
          printDecoratorNodeAsJson(dec, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != node.decorators.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }
      if node.pubToken |token| {
        stdoutWrite("$fieldsIndent\"pubToken\": ")
        printTokenAsJson(token, 0, currentIndentLevel + 1)
        stdoutWriteln(",")
      } else {
        stdoutWriteln("$fieldsIndent\"pubToken\": null,")
      }
      if node.isDecorator {
        stdoutWriteln("$fieldsIndent\"isDecorator\": true,")
      }
      stdoutWrite("$fieldsIndent\"typeName\": ")
      printLabelAsJson(node.name)

      if node.typeParams.isEmpty() {
        stdoutWriteln(",\n$fieldsIndent\"typeParams\": [],")
      } else {
        stdoutWriteln(",\n$fieldsIndent\"typeParams\": [")
        for label, idx in node.typeParams {
          stdoutWrite("$fieldsIndent  ")
          printLabelAsJson(label)
          val comma = if idx != node.typeParams.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }

      if node.fields.isEmpty() {
        stdoutWriteln("$fieldsIndent\"fields\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"fields\": [")
        for f, idx in node.fields {
          stdoutWriteln("$fieldsIndent  {")
          stdoutWrite("$fieldsIndent    \"label\": ")
          printLabelAsJson(f.name)
          if f.pubToken |token| {
            stdoutWrite(",\n$fieldsIndent    \"pubToken\": ")
            printTokenAsJson(token, 0, currentIndentLevel + 3)
          }
          stdoutWrite(",\n$fieldsIndent    \"typeAnnotation\": ")
          printTypeIdentifierAsJson(f.typeAnnotation, 0, currentIndentLevel + 3)
          stdoutWrite(",\n$fieldsIndent    \"initializer\": ")
          if f.initializer |initializer| {
            printAstNodeAsJson(initializer, 0, currentIndentLevel + 3)
          } else {
            stdoutWrite("null")
          }
          val comma = if idx != node.fields.length - 1 "," else ""
          stdoutWriteln("\n$fieldsIndent  }$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }

      if node.methods.isEmpty() {
        stdoutWriteln("$fieldsIndent\"methods\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"methods\": [")
        for method, idx in node.methods {
          printAstNodeKindAsJson(AstNodeKind.FunctionDeclaration(method), currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != node.methods.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }

      if node.types.isEmpty() {
        stdoutWriteln("$fieldsIndent\"nestedTypes\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"nestedTypes\": [")
        for nestedType, idx in node.types {
          printAstNodeKindAsJson(AstNodeKind.TypeDeclaration(nestedType), currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != node.types.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }

      if node.enums.isEmpty() {
        stdoutWriteln("$fieldsIndent\"nestedEnums\": []")
      } else {
        stdoutWriteln("$fieldsIndent\"nestedEnums\": [")
        for nestedEnum, idx in node.enums {
          printAstNodeKindAsJson(AstNodeKind.EnumDeclaration(nestedEnum), currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != node.types.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.EnumDeclaration(node) => {
      stdoutWriteln("$fieldsIndent\"name\": \"enumDeclaration\",")
      if node.decorators.isEmpty() {
        stdoutWriteln("$fieldsIndent\"decorators\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"decorators\": [")
        for dec, idx in node.decorators {
          printDecoratorNodeAsJson(dec, currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != node.decorators.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }
      if node.pubToken |token| {
        stdoutWrite("$fieldsIndent\"pubToken\": ")
        printTokenAsJson(token, 0, currentIndentLevel + 1)
        stdoutWriteln(",")
      } else {
        stdoutWriteln("$fieldsIndent\"pubToken\": null,")
      }
      stdoutWrite("$fieldsIndent\"typeName\": ")
      printLabelAsJson(node.name)

      if node.typeParams.isEmpty() {
        stdoutWriteln(",\n$fieldsIndent\"typeParams\": [],")
      } else {
        stdoutWriteln(",\n$fieldsIndent\"typeParams\": [")
        for label, idx in node.typeParams {
          stdoutWrite("$fieldsIndent  ")
          printLabelAsJson(label)
          val comma = if idx != node.typeParams.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }

      if node.variants.isEmpty() {
        stdoutWriteln("$fieldsIndent\"variants\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"variants\": [")
        for v, idx in node.variants {
          stdoutWriteln("$fieldsIndent  {")
          match v {
            EnumVariant.Constant(name) => {
              stdoutWriteln("$fieldsIndent    \"kind\": \"constant\",")
              stdoutWrite("$fieldsIndent    \"name\": ")
              printLabelAsJson(name)
            }
            EnumVariant.Container(name, fields) => {
              stdoutWriteln("$fieldsIndent    \"kind\": \"container\",")
              stdoutWrite("$fieldsIndent    \"name\": ")
              printLabelAsJson(name)
              if fields.isEmpty() {
                // unreachable
              } else {
                stdoutWriteln(",\n$fieldsIndent    \"fields\": [")
                for f, idx in fields {
                  stdoutWriteln("$fieldsIndent      {")
                  stdoutWrite("$fieldsIndent        \"label\": ")
                  printLabelAsJson(f.name)
                  stdoutWrite(",\n$fieldsIndent        \"typeAnnotation\": ")
                  printTypeIdentifierAsJson(f.typeAnnotation, 0, currentIndentLevel + 5)
                  stdoutWrite(",\n$fieldsIndent        \"initializer\": ")
                  if f.initializer |initializer| {
                    printAstNodeAsJson(initializer, 0, currentIndentLevel + 5)
                  } else {
                    stdoutWrite("null")
                  }
                  val comma = if idx != fields.length - 1 "," else ""
                  stdoutWriteln("\n$fieldsIndent      }$comma")
                }
                stdoutWrite("$fieldsIndent    ]")
              }
            }
          }
          val comma = if idx != node.variants.length - 1 "," else ""
          stdoutWriteln("\n$fieldsIndent  }$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }

      if node.methods.isEmpty() {
        stdoutWriteln("$fieldsIndent\"methods\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"methods\": [")
        for method, idx in node.methods {
          printAstNodeKindAsJson(AstNodeKind.FunctionDeclaration(method), currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != node.methods.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }

      if node.types.isEmpty() {
        stdoutWriteln("$fieldsIndent\"nestedTypes\": [],")
      } else {
        stdoutWriteln("$fieldsIndent\"nestedTypes\": [")
        for nestedType, idx in node.types {
          printAstNodeKindAsJson(AstNodeKind.TypeDeclaration(nestedType), currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != node.types.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent],")
      }

      if node.enums.isEmpty() {
        stdoutWriteln("$fieldsIndent\"nestedEnums\": []")
      } else {
        stdoutWriteln("$fieldsIndent\"nestedEnums\": [")
        for nestedEnum, idx in node.enums {
          printAstNodeKindAsJson(AstNodeKind.EnumDeclaration(nestedEnum), currentIndentLevel + 2, currentIndentLevel + 2)
          val comma = if idx != node.types.length - 1 "," else ""
          stdoutWriteln("$comma")
        }
        stdoutWriteln("$fieldsIndent]")
      }
    }
    AstNodeKind.Break => stdoutWriteln("$fieldsIndent\"name\": \"break\"")
    AstNodeKind.Continue => stdoutWriteln("$fieldsIndent\"name\": \"continue\"")
    AstNodeKind.Return(expr) => {
      stdoutWriteln("$fieldsIndent\"name\": \"return\",")

      if expr |expr| {
        stdoutWrite("$fieldsIndent\"expr\": ")
        printAstNodeAsJson(expr, 0, currentIndentLevel + 1)
        stdoutWriteln()
      } else {
        stdoutWriteln("$fieldsIndent\"expr\": null")
      }
    }
  }
  stdoutWrite("$endIndent}")
}
