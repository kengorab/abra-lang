type OneTwoThreeIterator {
  _count: Int = 1

  func next(self): Int? {
    val v = self._count
    if v > 3 {
      stdoutWriteln(process.getStackTrace())
      None
    } else {
      self._count += 1
      return Some(v)
    }
  }
}

val iter = OneTwoThreeIterator()
for i in iter {
  stdoutWriteln(i.toString())
}

