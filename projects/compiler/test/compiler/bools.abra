/// Expect: true
printlnv2(true)

/// Expect: false
printlnv2(false)

// Unary operation
/// Expect: true
printlnv2(!false)
/// Expect: false
printlnv2(!!false)
/// Expect: false
printlnv2(!true)
/// Expect: true
printlnv2(!!true)

// Binary operations
func returnsFalse(): Bool {
  printlnv2("boolFunc called")
  false
}

/// Expect: boolFunc called
/// Expect: false
printlnv2(true && returnsFalse())
/// Expect: false
printlnv2(false && returnsFalse())

/// Expect: true
printlnv2(true || returnsFalse())
/// Expect: boolFunc called
/// Expect: false
printlnv2(false || returnsFalse())

/// Expect: true
printlnv2(true ^ false)
/// Expect: true
printlnv2(false ^ true)
/// Expect: false
printlnv2(false ^ false)
/// Expect: false
printlnv2(true ^ true)

/// Expect: true
printlnv2(true == true)
/// Expect: false
printlnv2(true != true)
/// Expect: false
printlnv2(false == true)
/// Expect: true
printlnv2(true != false)
