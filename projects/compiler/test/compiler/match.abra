// Basic test (catch-all case)
(() => {
  /// Expect: here
  match "abc" {
    _ => printlnv2("here")
  }

  // v2 syntax
  /// Expect: here
  match "abc" {
    else => printlnv2("here")
  }
})()

// Testing constant cases
(() => {
  /// Expect: case 1: 1
  /// Expect: case 2: 2
  /// Expect: case 3: 34
  /// Expect: case 3: 567
  for i in [1, 2, 34, 567] {
    match i {
      1 v => printlnv2("case 1: $v")
      2 v => printlnv2("case 2: $v")
      _ v => printlnv2("case 3: $v")
    }
  }

  /// Expect: case 1: 1.1
  /// Expect: case 2: 2.2
  /// Expect: case 3: 3.4
  /// Expect: case 3: 5.67
  for f in [1.1, 2.2, 3.4, 5.67] {
    match f {
      1.1 v => printlnv2("case 1: $v")
      2.2 v => printlnv2("case 2: $v")
      _ v => printlnv2("case 3: $v")
    }
  }

  /// Expect: case 1: true
  /// Expect: case 2: false
  for b in [true, false] {
    match b {
      true v => printlnv2("case 1: $v")
      false v => printlnv2("case 2: $v")
    }
  }

  /// Expect: case 4: hello
  /// Expect: case 3: hi
  /// Expect: case 5: bonjour
  for s in ["hello", "hi", "bonjour"] {
    match s {
      "howdy" v => printlnv2("case 1: $v")
      "hola" v => printlnv2("case 2: $v")
      "hi" v => printlnv2("case 3: $v")
      "hello" v => printlnv2("case 4: $v")
      _ v => printlnv2("case 5: $v")
    }
  }

  // TODO: remove this, lambda is unhappy to have a for-loop be the last statement for some reason
  /// Expect: done
  printlnv2("done")
})()

// Testing option type
(() => {
  val arr = [1, 2, 3, 4]

  /// Expect: found a number
  match arr[1] {
    None => printlnv2("nothing here")
    else => printlnv2("found a number")
  }
  /// Expect: nothing here
  match arr[14] {
    None => printlnv2("nothing here")
    else => printlnv2("found a number")
  }

  // With case binding

  /// Expect: found a number: 2
  match arr[1] {
    None v => printlnv2("nothing here: $v")
    else v => printlnv2("found a number: $v")
  }
  /// Expect: nothing here: Option.None
  match arr[14] {
    None v => printlnv2("nothing here: $v")
    else v => printlnv2("found a number: $v")
  }

  // With case binding and _no_ `None` case

  /// Expect: value: Option.Some(value: 2)
  match arr[1] {
    else v => printlnv2("value: $v")
  }
  /// Expect: value: Option.None
  match arr[14] {
    else v => printlnv2("value: $v")
  }

  // With constant cases

  /// Expect: 18
  match arr[1] {
    2 v => printlnv2(16 + v)
    1 => printlnv2(15)
    None => printlnv2(-4)
    else => printlnv2(100)
  }
  /// Expect: 18
  match arr[1] {
    None => printlnv2(-4)
    2 v => printlnv2(16 + v)
    1 => printlnv2(15)
    else => printlnv2(100)
  }
  /// Expect: 18
  match arr[1] {
    2 v => printlnv2(16 + v)
    1 => printlnv2(15)
    else => printlnv2(100)
  }
  /// Expect: -4
  match arr[5] {
    2 v => printlnv2(16 + v)
    1 => printlnv2(15)
    None => printlnv2(-4)
    else => printlnv2(100)
  }
  /// Expect: -4
  match arr[5] {
    None => printlnv2(-4)
    2 v => printlnv2(16 + v)
    1 => printlnv2(15)
    else => printlnv2(100)
  }
  /// Expect: 100 Option.None
  match arr[5] {
    2 v => printlnv2(16 + v)
    1 => printlnv2(15)
    else v => printlnv2("${100} $v")
  }
  /// Expect: 100 Option.Some(value: 4)
  match arr[3] {
    2 v => printlnv2(16 + v)
    1 => printlnv2(15)
    else v => printlnv2("${100} $v")
  }

