// Testing match as expression
(() => {
  val arr = [1, 2, 3, 4]
  val x = match arr[1] {
    None => -1
    _ v => v * 100
  }
  /// Expect: 200
  printlnv2(x)

  val y = match arr[14] {
    None => -1
    _ v => v * 100
  }
  /// Expect: -1
  printlnv2(y)
})()

// Testing terminator in block

func matchExprCaseHasReturn(input: Int?): Int {
  val x = match input {
    None => return -1
    _ v => v * 100
  }
  x + 1
}
/// Expect: -1
printlnv2(matchExprCaseHasReturn(None))
/// Expect: 401
printlnv2(matchExprCaseHasReturn(Option.Some(value: 4)))

func matchStmtCaseHasReturn(input: Int?): Int {
  match input {
    None => return -1
    else v => printlnv2(v * 100)
  }

  1
}
/// Expect: -1
printlnv2(matchStmtCaseHasReturn(None))
/// Expect: 400
/// Expect: 1
printlnv2(matchStmtCaseHasReturn(Option.Some(value: 4)))
