match foo {
  1 => 12
  2 two => 3 + 4
  3.4 => "5"
  5.6 f => { [6, 7] }
  true => {
    val a = 8
    a + 9
  }
  false b => ten()
  None => {}
  None n => {}
}

val x = match foo() {
  Color.Blue => 0x0000FF
  colors.Color.Blue => 0x0000FF
  Color.Blue(a, b, c) => 0x0000FF
  _ => 0
  else f => f - 1
}

// special cases
match foo() {
  Ok(v) => v
  Err(e) => e
}
