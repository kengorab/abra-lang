/// Expect: 24
println(24)

// Unary operations
/// Expect: -1 1
println(-1, --1)

// Binary operations
/// Expect: 32
println(17 + 15)
/// Expect: 32.1
println(17 + 15.1)

/// Expect: 2
println(17 - 15)
/// Expect: 1.9
println(17 - 15.1)

/// Expect: 2550
println(17 * 150)
/// Expect: 2551.7
println(17 * 150.1)

/// Expect: 1.13333
println(17 / 15)
/// Expect: 1.12583
println(17 / 15.1)

/// Expect: 2
println(7 % 5)
/// Expect: 1.8
println(7 % 5.2)

/// Expect: 32
println(2 ** 5)
/// Expect: 0.03125
println(2 ** -5)
/// Expect: -32
println(-2 ** 5)
/// Expect: -0.03125
println(-2 ** -5)

/// Expect: true
println(1 << 3 != 17)
/// Expect: true
println(0x00ff << 8 == 0xff00)
/// Expect: true
println(0xff00 >> 8 == 0x00ff)

/// Expect: 1
println(0xff && 1)
/// Expect: 255
println(0xfe || 1)
/// Expect: 255
println(0b01010101 ^ 0b10101010)

/// Expect: true
println(17 == (15 + 2))
/// Expect: false
println(17 == 15)
