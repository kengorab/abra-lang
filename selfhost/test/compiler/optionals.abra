func f1(): Bool?[] = [None, Some(true), Some(false)]
/// Expect: [Option.None, Option.Some(value: true), Option.Some(value: false)]
println(f1())

func f2(): String?[] = if true { [Some("one"), None, Some("two")] } else []
/// Expect: [Option.Some(value: "one"), Option.None, Option.Some(value: "two")]
println(f2())

val arr1: Int?[] = [Some(1), None, Some(2), Some(3)]
/// Expect: [Option.Some(value: 1), Option.None, Option.Some(value: 2), Option.Some(value: 3)]
println(arr1)

val arr2: Int[]? = Some([1, 2, 3])
/// Expect: Option.Some(value: [1, 2, 3])
println(arr2)
val arr3: Int[]? = None
/// Expect: Option.None
println(arr3)

type Foo {
  f: String
}

// Workaround because optionals technically don't have _any_ directly-callable methods
func hash<T>(t: T): Int = t.hash()
// Hashes
// (() => {
func test_hashes() {
  val i: Int? = None
  /// Expect: 1
  println(hash(i))

  val x = Some(17)
  /// Expect: false
  println(hash(x) == (17).hash())
// })()
}
test_hashes()

// == operator
// (() => {
func test_eqOp() {
  // Test with primitive
  val i1: Int? = None
  val i2 = Some(17)
  val i3 = Some(17)
  val i4 = Some(16)

  /// Expect: false
  println(i1 == i2)
  /// Expect: false
  println(i2 == i4)
  /// Expect: true
  println(i2 == i3)

  // Test with object
  val s1: String? = None
  val s2 = Some("str")
  val s3 = Some("str")
  val s4 = Some("hmm")

  /// Expect: false
  println(s1 == s2)
  /// Expect: false
  println(s2 == s4)
  /// Expect: true
  println(s2 == s3)
// })()
}
test_eqOp()

